{
  "name": "expressjs-typescript-template",
  "version": "1.0.0",
  "description": "latest Template For Express",
  "main": "src/index.ts",
  "scripts": {
    "//start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start": "node -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "watch": "tsc -w",
    "server": "tsc-watch --project . --outDir ./dist --onSuccess \"nodemon ./dist/index.js\""
  },
  "dependencies": {
    "@types/cors": "2.8.13",
    "@types/uuid": "9.0.0",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "simple-git": "3.15.1",
    "tsc-watch": "^6.0.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.15",
    "@types/node": "18.11.15",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typescript": "^4.9.0"
  },
  "keywords": [
    "template"
  ]
}
